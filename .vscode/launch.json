    "version": "0.2.0",
    "configurations": [
        {
            "name": "evaluation run pythia-160m",
            "type": "debugpy",
            "request": "launch",
            "program": "src/evaluation/run.py",
            "console": "integratedTerminal",
            "args": [
                // "--model_name=HuggingFaceM4/Idefics3-8B-Llama3", "-mm",
                "--model_name=EleutherAI/pythia-160m", 
                "--tasks=xml_numericnlg", 
                // "--tasks=image_comtqa_pmc", 
                "--num_fewshot=0", 
                // "--device=cuda:0", 
                "--output_path=./results", "--seed=42", "--log_samples",  
                // "--use_chat_template", // not supported by all models
                "--task_path=./src/evaluation/tasks",
                "--limit_samples=1",
                "--data_base_path=../data/TableEval"
            ]
        },
        {
            "name": "evaluation run gpt4o-mini",
            "type": "debugpy",
            "request": "launch",
            "program": "src/evaluation/run.py",
            "console": "integratedTerminal",
            "args": [
                "--model_name=azure/gpt-4o-mini-real", 
                "--model_args={'api_base':'https://gpt4-se-dev.openai.azure.com/','api_version':'2024-08-01-preview','azure_ad_token':'bearer_token'}",
                "--api_model",
                "--tasks=xml_numericnlg", 
                "--num_fewshot=0", 
                "--output_path=./results", "--seed=42", "--log_samples",  
                "--task_path=./src/evaluation/tasks",
                "--data_base_path=../data/TableEval",
                // "--limit_samples=1",
                // "--use_chat_template", // not supported by all models
            ]
        }
    ]
}